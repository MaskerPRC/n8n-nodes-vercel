{
	"name": "n8n-nodes-vercel",
	"version": "0.6.0",
	"description": "n8n community node for deploying static HTML sites to Vercel",
	"license": "MIT",
	"homepage": "https://github.com/n8n-io/n8n-nodes-starter",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"n8n-node",
		"vercel",
		"deployment"
	],
	"author": {
		"name": "",
		"email": ""
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/n8n-io/n8n-nodes-starter.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/GithubIssuesApi.credentials.js",
			"dist/credentials/GithubIssuesOAuth2Api.credentials.js",
			"dist/credentials/VercelApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/GithubIssues/GithubIssues.node.js",
			"dist/nodes/Example/Example.node.js",
			"dist/nodes/Vercel/Vercel.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@types/archiver": "^7.0.0",
		"@types/node": "^24.10.1",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"archiver": "^7.0.1",
		"form-data": "^4.0.0"
	}
}
